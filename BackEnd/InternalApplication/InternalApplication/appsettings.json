{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Log\\log-{Date}.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}",
          "fileSizeLimitBytes": "1000000"
        }
      },
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "https://localhost:44357" }
      }
    ]
  },
  "JwtSecurityToken": {
    "SecretKey": "InternalApplication@2019!#$0919",
    "Issuer": "InternalApplication",
    "Audience": "InternalApplication",
    "AccessTokenLifetime": 1440
  },
  "ConnectionStrings": {
    "InternalConnection": "Server=TODO; Database=TODO; User Id=TODO; password=TODO@TODO@TODO#!;"
  },
  "AllowedHosts": "*"

}
